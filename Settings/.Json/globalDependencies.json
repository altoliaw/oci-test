{
	"dependencies": [
		{
			"name": "libpcap",
			"download": "wget -O {{name}}/libpcap-1.9.1.tar.gz https://www.tcpdump.org/release/libpcap-1.9.1.tar.gz",
			"command": "tar zxvf libpcap-1.9.1.tar.gz && cd libpcap-1.9.1 && ./configure --prefix={{projectVendors}}/{{name}} && make && make install && cd ..",
			"includes": [
				"include/*"
			],
			"libs": [
				"lib/*"
			],
			"others": [
			],
			"reference": "https://www.tcpdump.org/release/",
			"remove": "rm -rf {{name}}",
			"windows": false,
			"linux": true
		},
		{
			"name": "cJson",
			"download": "git clone https://github.com/DaveGamble/cJSON.git {{name}}",
			"command": "make",
			"includes": [
				"cJSON.h"
			],
			"libs": [
				"libcjson.a"
			],
			"others": [
			],
			"reference": "https://github.com/DaveGamble/cJSON",
			"remove": "rm -rf {{name}}",
			"windows": true,
			"linux": true
		},
		{
			"name": "nPcap",
			"download": "wget -O {{name}}/tmp.zip https://npcap.com/dist/npcap-sdk-1.13.zip",
			"command": "unzip tmp.zip",
			"includes": [
				"Include/*"
			],
			"libs": [
				"Lib/*"
			],
			"others": [
			],
			"reference": "https://npcap.com/#download",
			"remove": "rm -rf {{name}}",
			"windows": true,
			"linux": false
		}, 
		{
			"name": "OCI",
			"download": "wget -O {{name}}/basic.rpm https://yum.oracle.com/repo/OracleLinux/OL8/oracle/instantclient/x86_64/getPackage/oracle-instantclient19.10-basic-19.10.0.0.0-1.x86_64.rpm && wget -O {{name}}/devel.rpm https://yum.oracle.com/repo/OracleLinux/OL8/oracle/instantclient/x86_64/getPackage/oracle-instantclient19.10-devel-19.10.0.0.0-1.x86_64.rpm",
			"command": "rpm2cpio basic.rpm | cpio -idmv && rpm2cpio devel.rpm | cpio -idmv && rm -rf $(pwd)/usr/lib/oracle/19.10/client64/lib/network glogin.sql ojdbc8.jar ottclasses.zip xstreams.jar",
			"includes": ["./usr/include/oracle/19.10/client64/*"],
			"libs": ["./usr/lib/oracle/19.10/client64/lib/*"],
			"others": [],
			"reference": "https://yum.oracle.com/repo/OracleLinux/OL8/oracle/instantclient/x86_64/index.html",
			"remove": "rm -rf {{name}}",
			"windows": false,
			"linux": true
		},
		{
			"name": "ocilib",
			"download": "git clone https://github.com/vrogier/ocilib.git {{name}}",
			"command": "./configure --with-oracle-headers-path=$(pwd)/../OCI/Includes/ --with-oracle-lib-path=$(pwd)/../OCI/Libs/ --prefix=$(pwd)/temp && make && make install",
			"includes": ["temp/include/*"],
			"libs": ["temp/lib/*"],
			"others": [],
			"reference": "https://github.com/vrogier/ocilib.git",
			"remove": "rm -rf {{name}}",
			"windows": false,
			"linux": true
		}
	]
}